{
  "configurations": [
    {
      "inheritEnvironments": [
        "msvc_x64"
      ],
      "name": "x64-Debug",
      "includePath": [
        "${env.INCLUDE}",
        "${workspaceRoot}\\**"
      ],
      "defines": [
        "WIN32",
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "_MSVC_LANG=202002L"
      ],
      "intelliSenseMode": "windows-msvc-x64",
      "cppStandard": "c++20",
      "cStandard": "c17",
      "compilerSwitches": "/Zc:__cplusplus /std:c++20"
    }
  ]
}